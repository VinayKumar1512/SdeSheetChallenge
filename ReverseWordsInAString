public class Solution 
{
	public static String reverseString(String str) 
	{
		StringBuilder sb= new StringBuilder();
        
        String arr[]= str.split(" ");
        for(int i=arr.length-1;i>=0;i--){
            sb.append(arr[i]);
            sb.append(" "); 
        }
        
        return sb.toString();
	}
}
